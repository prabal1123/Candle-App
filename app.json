{
  "expo": {
    "name": "candle-app",
    "slug": "candle-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "candleapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.okayprabal.candleapp"
    },
    "android": {
      "package": "com.okayprabal.candleapp",
      "minSdkVersion": 23,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "SUPABASE_URL": "https://bybxickqlfiirrjkvuoq.supabase.co",
      "SUPABASE_ANON_KEY": "YOUR_ANON_KEY",
      "router": {},
      "eas": {
        "projectId": "cfe3a455-9f8d-46c1-a771-aaf310377dcc"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cfe3a455-9f8d-46c1-a771-aaf310377dcc"
    }
  }
}
